{
  "name": "merge-branch-helper",
  "displayName": "Merge Branch Helper",
  "description": "自动将当前分支合并到指定分支并返回",
  "version": "0.0.1",
  "publisher": "wangshaoh",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "git",
    "merge",
    "branch",
    "自动合并",
    "分支管理"
  ],
  "icon": "images/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/你的用户名/merge-branch-helper.git"
  },
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "merge-branch-helper.mergeToBranch",
        "title": "合并到指定分支"
      }
    ],
    "configuration": {
      "title": "Merge Branch Helper",
      "properties": {
        "mergeBranchHelper.targetBranch": {
          "type": "string",
          "default": "master",
          "description": "目标分支名称"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.85.0",
    "@types/node": "18.x",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3"
  }
} 